{
  "extends": "airbnb-base",
  "rules": {
    "func-names": 0,
    "function-paren-newline": 0,
    "no-use-before-define": 0,
    "no-confusing-arrow": 0,
    "arrow-parens": 0,
    "strict": 0,
    "spaced-comment": 0,
    "prefer-template": 0,
    "comma-dangle": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "quotes": 0,
    "no-mixed-operators": 0,
    "no-param-reassign": 0,
    "no-console": 0,
    "max-len": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "prefer-destructuring": 0,
    "import/no-dynamic-require": 0
  }
}
